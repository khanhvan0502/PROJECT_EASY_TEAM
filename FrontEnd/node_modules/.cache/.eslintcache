roup\">\r\n                <h5>\r\n                  Xác nhận mật khẩu{\" \"}\r\n                  <b style={{ color: \"red\", fontSize: \"24px\" }}>*</b>\r\n                </h5>\r\n                <input\r\n                  type=\"password\"\r\n                  placeholder=\"Nhập lại mật khẩu mới\"\r\n                  className=\"confirm-password\"\r\n                  name=\"confirm_password\"\r\n                  onChange={handleInput}\r\n                  value={passwordList.confirm_password}\r\n                />\r\n                <span>{passwordList.error_list}</span>\r\n              </div>\r\n\r\n              <div className=\"btn-change-password\">\r\n                <button className=\"btn-change-password form-control-submit-button\">\r\n                  Đổi mật khẩu\r\n                </button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </>\r\n    );\r\n  });\r\n  return (\r\n    <>\r\n      <header className=\"ex-header\">\r\n        <div className=\"container\">\r\n          <div className=\"row\">\r\n            <div className=\"col-xl-10 offset-xl-1\">\r\n              <h1 className=\"text-center\">Đổi mật khẩu</h1>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </header>\r\n\r\n      {showProfile}\r\n      <Footer />\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ChangePassword;\r\n","D:\\xampp\\htdocs\\OFFICAL_PROJECT\\PROJECT_EASY_TEAM\\FrontEnd\\src\\components\\admin\\CategoryQuiz\\EditCategoryQuiz.js",[],"D:\\xampp\\htdocs\\OFFICAL_PROJECT\\PROJECT_EASY_TEAM\\FrontEnd\\src\\components\\admin\\Profile\\EditProfile.js",[],"D:\\xampp\\htdocs\\OFFICAL_PROJECT\\PROJECT_EASY_TEAM\\FrontEnd\\src\\components\\admin\\Item\\AddItem.js",[],"D:\\xampp\\htdocs\\OFFICAL_PROJECT\\PROJECT_EASY_TEAM\\FrontEnd\\src\\components\\admin\\CategoryQuiz\\AddCategoryQuiz.js",[],"D:\\xampp\\htdocs\\OFFICAL_PROJECT\\PROJECT_EASY_TEAM\\FrontEnd\\src\\components\\admin\\Item\\ViewItem.js",[],"D:\