n          setErrorList([]);\r\n        } else {\r\n          swal(\"All Fields are mandatory\", \"\", \"error\");\r\n          setErrorList(res.data.errors);\r\n        }\r\n      });\r\n    });\r\n  };\r\n\r\n\r\n  return (\r\n    <div>\r\n      <Navbar />\r\n      <header className=\"ex-header\">\r\n        <div className=\"container\">\r\n          <div className=\"row\">\r\n            <div className=\"col-xl-10 offset-xl-1\">\r\n              <h1 className=\"text-center\">Đặt câu hỏi</h1>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </header>\r\n      <div className=\"question-container\">\r\n        <div className=\"hint\">\r\n          <p>\r\n            {\" \"}\r\n            Hãy đặt câu hỏi phỏng vấn liên quan đến công việc của bạn sắp tới.\r\n            Lưu ý,{\" \"}\r\n            <b\r\n              style={{\r\n                color: \"red\",\r\n                fontSize: \"24px\",\r\n              }}\r\n            >\r\n              *\r\n            </b>{\" \"}\r\n            là thành phần bắt buộc\r\n          </p>\r\n        </div>\r\n        <div className=\"main-form\">\r\n          <form onSubmit={submitQuestion} encType=\"multipart/form-data\">\r\n            <div className=\"form-group\">\r\n              <label>\r\n                Tiêu đề{\" \"}\r\n                <b\r\n                  style={{\r\n                    color: \"red\",\r\n                    fontSize: \"24px\",\r\n                  }}\r\n                >\r\n                  *\r\n                </b>\r\n              </label>\r\n              <input\r\n                type=\"text\"\r\n                name=\"title\"\r\n                onChange={handleInput}\r\n                value={questionInput.title}\r\n                className=\"form-control\"\r\n                placeholder=\"Nhập tiêu đề câu hỏi\"\r\n              />\r\n            </div>\r\n\r\n            <div className=\"note\">\r\n              <p>\r\n                Vui lòng đặt tiêu đề thích 